  self~assertEquals("subtest02-e", "2v", a~"[]"(idx))
  self~assertTrue("subtest02-f", a~hasindex(1,2,1))
  self~assertTrue("subtest02-g", a~hasindex(idx))

  idx=.array~of(3,1,1)
