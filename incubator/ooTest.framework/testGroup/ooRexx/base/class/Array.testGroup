::method "test_difference"
  expose collDir clz bUserDefinedIndex bSingleItem

  ce=collDir~emptyColl
  c1=collDir~coll_1
  c2=collDir~coll_2

  self~assertEquals("subtest1", ce, ce~difference(ce))

  self~assertEquals("subtest2", c1, c1~difference(ce))
  self~assertEquals("subtest3", c2, c2~difference(ce))

  self~assertEquals("subtest4", collDir~difference1, c1~difference(c2))
  self~assertEquals("subtest5", collDir~difference2, c2~difference(c1))

  self~assertEquals("subtest6", ce, c1~difference(c1))
  self~assertEquals("subtest7", ce, c2~difference(c2))


   -- now test where other is an 'OrderedCollection': "makeArray" returns "allItems"
  o1=.object~new
  c =clz~new~~put("1", "101")~~put("2", "102")~~put(o1, "103")
  other=.array~new
  other[104]="2"
  other[106]=o1
  d1=clz~of("1")     -- expected result
  d2=c~difference(other)
  self~assertTrue("subtest8: 'other' is an 'OrderedCollection'", sameContent(d1, d2))

  -- Simplistic .Array difference .Table test
  receiverArray = .array~new
  receiverArray[1] = 'Lemon'
  receiverArray[2] = 'Orange'
  receiverArray[3] = 'Dogwood'

  argumentTable = .table~new
  argumentTable[1] = 'Dogwood'
  argumentTable[2] = 'Elm'
  argumentTable[3] = 'Rose'
  argumentTable[4] = 'Tulip'

  resultObj = receiverArray~difference(argumentTable)

  nl = '0d0a'
  self~assertTrue("After set-like operation (intersection) result object must be array", resultObj~isA(.array))
  self~assertSame('Intersection of Lemon Orange Dogwood with Dogwood Elm Rose Tulip' || nl || -
                  'should have 2 items', 2, resultObj~items)
  self~assertTrue('Intersection result should have a Lemon item', resultObj~hasItem('Lemon'))
  self~assertTrue('Intersection result should have an Orange item', resultObj~hasItem('Orange'))




