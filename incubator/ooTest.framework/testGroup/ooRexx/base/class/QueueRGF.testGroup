::method "test_remove"
  expose collDir clz bUserDefinedIndex bSingleItem o1 o2

  ce=collDir~emptyColl~copy
  c1=collDir~coll_1~copy

  self~assertNull("subtest01", ce~remove(98))

  self~assertNull("subtest02", c1~remove(99))

   -- if removing an item, all successive items move up to the top
  self~assertEquals("subtest03", "1v", c1~remove("1"))
  self~assertEquals("subtest04", "2v", c1~remove("1"))
  self~assertEquals("subtest05", "2v", c1~remove("1"))
  self~assertEquals("subtest06", "2v", c1~remove("1"))
  self~assertEquals("subtest07", o1, c1~remove("1"))
  self~assertEquals("subtest08", o1, c1~remove("1"))
  self~assertNull(  "subtest09", c1~remove("1"))
  self~assertEquals("subtest10", 0, c1~items)

  -- now doing it in reverse order
  c1=collDir~coll_1~copy

  self~assertEquals("subtest11", 6, c1~items)

  self~assertEquals("subtest12", o1, c1~remove("6"))
  self~assertNull(  "subtest13", c1~remove("6"))

  self~assertEquals("subtest14", o1, c1~remove("5"))
  self~assertNull(  "subtest15", c1~remove("5"))

  self~assertEquals("subtest16", "2v", c1~remove("4"))
  self~assertNull(  "subtest17", c1~remove("4"))

  self~assertEquals("subtest18", "2v", c1~remove("3"))
  self~assertNull(  "subtest19", c1~remove("3"))

  self~assertEquals("subtest20", "2v", c1~remove("2"))
  self~assertNull(  "subtest21", c1~remove("2"))

  self~assertEquals("subtest22", "1v", c1~remove("1"))
  self~assertNull(  "subtest23", c1~remove("1"))

  self~assertEquals("subtest24", 0, c1~items)



::method "test_removeItem"
  expose collDir clz bUserDefinedIndex bSingleItem o1 o2

  ce=collDir~emptyColl~copy
  c1=collDir~coll_1~copy

  self~assertNull("subtest01", ce~removeItem("1"))
  self~assertNull("subtest02", ce~removeItem(.nil))

  self~assertNull("subtest03", c1~removeItem("99"))

  self~assertEquals("subtest04", "1v", c1~removeItem("1v"))
  self~assertNull(  "subtest05", c1~removeItem("1v"))

  self~assertEquals("subtest06", "2v", c1~removeItem("2v"))
