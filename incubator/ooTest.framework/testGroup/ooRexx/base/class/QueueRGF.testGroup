::method "test_difference"
  expose collDir clz bUserDefinedIndex bSingleItem

  ce=collDir~emptyColl
  c1=collDir~coll_1
  c2=collDir~coll_2

  self~assertEquals("subtest1", ce, ce~difference(ce))

  self~assertEquals("subtest2", c1, c1~difference(ce))
  self~assertEquals("subtest3", c2, c2~difference(ce))

  self~assertEquals("subtest4", collDir~difference1, c1~difference(c2))
  self~assertEquals("subtest5", collDir~difference2, c2~difference(c1))

  self~assertEquals("subtest6", ce, c1~difference(c1))
  self~assertEquals("subtest7", ce, c2~difference(c2))


   -- now test where other is an 'OrderedCollection': "makeArray" returns "allItems"
  o1=.object~new
  c =clz~new~~append(o1)~~append("1")~~append("2")
  other=.array~new
  other[104]="2"
  other[106]=o1
  d1=clz~of("1")     -- expected result
  d2=c~difference(other)
  self~assertTrue("subtest8: 'other' is an 'OrderedCollection'", sameContent(d1, d2))

  -- Simplistic test of queue difference table
  receiverQ = .queue~new
  do i = 1 to 4
    receiverQ~push(i)
  end

  argumentTable = .table~new
  do i = 1 to 5 by 2
    argumentTable[i] = i
  end

  resultObj = receiverQ~difference(argumentTable)

  self~assertSame("(1.) result object after difference operation must be the same class as the receiver", .queue, resultObj~class)
  self~assertSame("1 2 3 4 difference 1 3 5 should result in a collection of 2 items", 2, resultObj~items)
  self~assertTrue("1 2 3 4 difference 1 3 5 result should have an item of 2", resultObj~hasItem(2))
  self~assertTrue("1 2 3 4 difference 1 3 5 result should have an item of 4", resultObj~hasItem(4))



