/*  The index value of elements inserted into a list  will be incremented
    by one, but the order in which MAKEARRAY will deliver the index values
    will be in list order.
*/
::method "test_insert"
  expose collDir clz bUserDefinedIndex bSingleItem o1 o2

  a=clz~new
  a~insert("a")      -- insert at end
  self~assertTrue(  "subtest_01a", a~hasItem("a"))
  self~assertTrue(  "subtest_01b", a~hasIndex(0))
  self~assertEquals("subtest_01c", "a", a[0])

  a~insert("b")      -- insert at end
  self~assertTrue(  "subtest_02a", a~hasItem("b"))
  self~assertTrue(  "subtest_02b", a~hasIndex(1))
  self~assertEquals("subtest_02c", "a", a[0])
  self~assertEquals("subtest_02d", "b", a[1])

  a~insert("c",0)    -- insert after first element
  self~assertTrue(  "subtest_03a", a~hasItem("c"))
  self~assertTrue(  "subtest_03b", a~hasIndex(2))

  self~assertEquals("subtest_03c", "a", a[0])
  self~assertEquals("subtest_03d", "b", a[1])
  self~assertEquals("subtest_03e", "c", a[2])

  a~insert("d",.nil) -- insert as first


  self~assertTrue(  "subtest_04a", a~hasItem("c"))
  self~assertTrue(  "subtest_04b", a~hasIndex(2))
  self~assertEquals("subtest_04c", "a", a[0])
  self~assertEquals("subtest_04d", "b", a[1])
  self~assertEquals("subtest_04e", "c", a[2])
  self~assertEquals("subtest_04f", "d", a[3])






