::method "test_PUT"
  expose collDir clz bUserDefinedIndex bSingleItem

  tmpColl=clz~new
  dummy="test"
  self~assertEquals("subtest01", 0, tmpColl              ~items)

  idx=0
  ind=2
  item="1"
  items=1
  self~assertFalse( "subtest0"ind"a", tmpColl~hasindex(idx))
  self~assertFalse( "subtest0"ind"b", tmpColl~hasitem(item))
  tmpColl~insert(dummy)
  self~assertTrue(  "subtest0"ind"c", tmpColl~hasitem(dummy))
  self~assertTrue(  "subtest0"ind"d", tmpColl~hasindex(idx))
  self~assertEquals("subtest0"ind"e", 1, tmpColl~~put(item,idx)~items)
  self~assertTrue(  "subtest0"ind"f", tmpColl~hasindex(idx))
  self~assertTrue(  "subtest0"ind"g", tmpColl~hasitem(item))
  self~assertFalse( "subtest0"ind"h", tmpColl~hasitem(dummy))

  idx=1
  ind=3
  item="2"
  items=2
  self~assertFalse( "subtest0"ind"a", tmpColl~hasindex(idx))
  self~assertFalse( "subtest0"ind"b", tmpColl~hasitem(item))
  tmpColl~insert(dummy)
  self~assertTrue(  "subtest0"ind"c", tmpColl~hasitem(dummy))
  self~assertTrue(  "subtest0"ind"d", tmpColl~hasindex(idx))
  self~assertEquals("subtest0"ind"e", 2, tmpColl~~put(item,idx)~items)
  self~assertTrue(  "subtest0"ind"f", tmpColl~hasindex(idx))
  self~assertTrue(  "subtest0"ind"g", tmpColl~hasitem(item))
  self~assertFalse( "subtest0"ind"h", tmpColl~hasitem(dummy))

  idx=1
  ind=4
  item="3"
  items=2
  self~assertTrue(  "subtest0"ind"d", tmpColl~hasindex(idx))
  self~assertEquals("subtest0"ind"e", 2, tmpColl~~put(item,idx)~items)
  self~assertTrue(  "subtest0"ind"f", tmpColl~hasindex(idx))
  self~assertTrue(  "subtest0"ind"g", tmpColl~hasitem(item))
  self~assertFalse( "subtest0"ind"h", tmpColl~hasitem(dummy))

  idx=1
  ind=5
  item="2"
  items=2
  self~assertTrue(  "subtest0"ind"d", tmpColl~hasindex(idx))
  self~assertEquals("subtest0"ind"e", 2, tmpColl~~put(item,idx)~items)
  self~assertTrue(  "subtest0"ind"f", tmpColl~hasindex(idx))
  self~assertTrue(  "subtest0"ind"g", tmpColl~hasitem(item))
  self~assertFalse( "subtest0"ind"h", tmpColl~hasitem(dummy))



  idx=2
  ind=6
  item="3"
  items=3
  self~assertFalse( "subtest0"ind"a", tmpColl~hasindex(idx))
  self~assertFalse( "subtest0"ind"b", tmpColl~hasitem(item))
  tmpColl~insert(dummy)
  self~assertTrue(  "subtest0"ind"c", tmpColl~hasitem(dummy))
  self~assertTrue(  "subtest0"ind"d", tmpColl~hasindex(idx))
  self~assertEquals("subtest0"ind"e", 3, tmpColl~~put(item,idx)~items)
  self~assertTrue(  "subtest0"ind"f", tmpColl~hasindex(idx))
  self~assertTrue(  "subtest0"ind"g", tmpColl~hasitem(item))
  self~assertFalse( "subtest0"ind"h", tmpColl~hasitem(dummy))

  self~assertTrue("subtest07", sameContent(.bag~of("1","2","3"), tmpColl))



