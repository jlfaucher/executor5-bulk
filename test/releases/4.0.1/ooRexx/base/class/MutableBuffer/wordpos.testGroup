   -- test the BIF, using examples from the documentation
::method "test_WORDPOS"
    self~assertEquals(3, .mutablebuffer~new('now is the time')~wordPos('the'))
    self~assertEquals(0, .mutablebuffer~new('now is the time')~wordPos('The'))
    self~assertEquals(2, .mutablebuffer~new('now is the time')~wordPos('is the'))
    self~assertEquals(2, .mutablebuffer~new('now is the time')~wordPos('is the'))
    self~assertEquals(0, .mutablebuffer~new('now is the time')~wordPos('is time '))
    self~assertEquals(2, .mutablebuffer~new('To be or not to be')~wordPos('be'))
    self~assertEquals(6, .mutablebuffer~new('To be or not to be')~wordPos('be',3))


