::requires 'ooTest.frm' -- load the ooRexxUnit classes

::class 'substrSuite' subclass ooTestSuite public
::method setUp
    .local~vblank = ' '
    .local~v1 = '*'
    .local~v30 = 'abcdefghijklmnopqrstuvwxyz1234'
    .local~v256 = copies('12345678',32)
    .local~v10000 = copies('\',10000)

::method tearDown
    .local~remove(vblank)
    .local~remove(v1)
    .local~remove(v30)
    .local~remove(v256)
    .local~remove(v10000)


::class "substr.testGroup" subclass ooTestCase public

::method 'test001'
    mb = .mutablebuffer~new()
    self~expectSyntax(93.924)
    xre = .vblank~substr(0)

::method 'test002'
