/* class: SysStemCopy.testGroup  - - - - - - - - - - - - - - - - - - - - - - -*\

\* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/
::class "SysStemCopy.testGroup" public subclass ooTestCase

::method test_copies

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 2))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'b', 'c', 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 4))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'd', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 2, 1))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'b', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.', 2,, 0))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.',,,,'o'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 2,,'O'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'd', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 2, 1, 'Overlay'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'b', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.', 2,, 0, 'o'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'b', 'c', 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 4,, 'o'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'd', 'e', 'f', 'a', 'b', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.',,,,'i'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'd', 'e', 'f', 'b', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 2,,'I'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'd', 'b', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 2, 1, 'Insert'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'b', 'c')
  self~assertEquals(0, SysStemCopy(b., 'a.', 2,, 0, 'i'))
  self~verifySeq(a., e.)

  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  self~of(e., 'a', 'b', 'c', 'd', 'e', 'f')
  self~assertEquals(0, SysStemCopy(b., 'a.',, 4,, 'i'))
  self~verifySeq(a., e.)

::method test_unset_to_count
  a.1 = 'a'
  self~of('b.', 'c')
  self~expectSyntax("40.922")
  rv = SysStemCopy(a., b.)

::method test_bad_to_count
  a.0 = '*'
  a.1 = 'a'
  self~of('b.', 'c')
  self~expectSyntax("40.922")
  rv = SysStemCopy(a., b.)

::method test_unset_from_count
  a.1 = 'a'
  self~of('b.', 'c')
  self~expectSyntax("40.922")
  rv = SysStemCopy(b., a.)

::method test_bad_from_count
  a.0 = '*'
  a.1 = 'a'
  self~of('b.', 'c')
  self~expectSyntax("40.922")
  rv = SysStemCopy(b., a.)

