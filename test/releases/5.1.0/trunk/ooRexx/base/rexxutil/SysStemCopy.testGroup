  self~of(b., 'd', 'e', 'f')
  self~expectSyntax("40.923")
  rv = SysStemCopy('a.',b.,,,4)

::method test_invalid_option
  self~of(a., 'a', 'b', 'c')
  self~of(a., 'd', 'e', 'f')
  self~expectSyntax("40.920")
  rv = SysStemCopy('a.',b.,,,,'d')

::method test_sparse_insert_target
  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  drop b.2
  self~expectSyntax("40.924")
  rv = SysStemCopy('a.',b.,,,,'i')

::method test_sparse_insert_source
  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  drop a.2
  self~expectSyntax("40.924")
  rv = SysStemCopy('a.',b.,,,,'i')

::method test_sparse_overlay_source
  self~of(a., 'a', 'b', 'c')
  self~of(b., 'd', 'e', 'f')
  drop a.2
  self~expectSyntax("40.924")
  rv = SysStemCopy('a.',b.,,,,'o')




::method verifySeq
  use arg source., result.

  do i=0 to result.0
     self~assertEquals(source.i, result.i)
  end

::method of
  use arg target.
  values = arg(2, 'a')

  target.0 = values~items
  do i = 1 to target.0
      target.i = values[i]
  end

::method dumpstem
  use arg a.
  say "size is" a.0

  do i = 1 to a.0
      say "Item" i "is" a.i
  end

-- End of class: SysStemSort.testGroup


::options novalue error

