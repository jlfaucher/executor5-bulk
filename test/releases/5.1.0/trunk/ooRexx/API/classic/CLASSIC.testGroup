  do data over "test data", "", " ", '00'x, "contains" || '00'x || "null"
    -- add to a named queue
    t1~testaddqueue(q1, data, 0) -- 0 = FIFO
    self~assertEquals(0, t1~retc)
    -- pull from a named queue
    t1~testpullfromqueue(q1)
    self~assertEquals(0, t1~retc)
    self~assertEquals(data, t1~flag) -- flag is the pulled data
  end

