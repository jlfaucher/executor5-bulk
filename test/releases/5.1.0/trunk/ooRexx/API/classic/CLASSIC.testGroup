-- test SHVBLOCK P/PRIV, direct interface
::method test_shv_private
  parse source s
  parse version v
  private = .Stringtable~of( -
   ("QUENAME", rxqueue("GET")), ("SOURCE", s), ("VERSION", v))
  do with index name item value over private
    shv = .Shv~new("P", name)
    self~assertSame(.Shv~OK, TestFVariablePool(shv), name)
    self~assertSame(value, shv~shvvalue)
  end

  -- this method receives no args, so we test PARM and PARM.n by calling
  -- method shv_private_args with one arg (our Array of SHVBLOCKs)
  -- we use a chained request here
  parms = .Shv~new("P", "PARM"), .Shv~new("P", "PARM.1"), .Shv~new("P", "PARM.2")
  self~assertSame(.Shv~OK, self~shv_private_args(parms))
  self~assertSame(1, parms[1]~shvvalue)
  self~assertSame("an Array", parms[2]~shvvalue)
  self~assertSame("", parms[3]~shvvalue) -- omitted args return nullstring

  -- variable value truncation returns TRUNC
  do with index name item value over private~~remove("QUENAME")
    shv = .Shv~new("P", name, , 8) -- 8 is too small for SOURCE and VERSION
    self~assertSame(.Shv~TRUNC, TestFVariablePool(shv), name)
    self~assertSame(value~left(8), shv~shvvalue)
  end

  -- lowercase or invalid variable names return BADN
  do name over "source", "P", "PARM.ALL", "ARG"
    self~assertSame(.Shv~BADN, TestFVariablePool(.Shv~new("P", name)), name)
  end


::method shv_private_args
  return TestFVariablePool(arg(1))

-- test SHVBLOCK N/NEXTV, direct interface
::method test_shv_next
  shv = .Array~new(9)
  do shv~size
    shv~append(.Shv~new("N", ""))
  end

  -- we now have four variables defined: SELF, SUPER, SHV, and RESULT
  -- we have n+1 SHVBLOCKs and the last one must be LVAR
  self~assertSame(5, TestFNVariablePool(shv))
  self~assertSame(.Shv~LVAR, shv[5]~shvret) -- last+1 SHVBLOCK must be LVAR
  mustHave = "SELF", "SUPER", "SHV", "RESULT"
  do i = 1 to mustHave~items
    self~assertSame(.Shv~OK, shv[i]~shvret)
    self~assertTrue(mustHave~hasItem(shv[i]~shvname), shv[i]~shvname)
    mustHave~removeItem(shv[i]~shvname)
    select case shv[i]~shvname
      when "SHV"    then self~assertSame("an Array", shv[i]~shvvalue)
      when "RESULT" then self~assertSame(9, shv[i]~shvvalue)
      otherwise nop
    end
  end

  -- call a procedure where not a single variable is defined
  self~assertSame(1, procedureVariablePool(shv))
  self~assertSame(.Shv~LVAR, shv[1]~shvret) -- last+1 SHVBLOCK must be LVAR
  return

  procedureVariablePool: procedure
  return TestFNVariablePool(arg(1))


