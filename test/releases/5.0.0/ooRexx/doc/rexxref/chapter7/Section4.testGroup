::requires 'ooTest.frm'

::class "Section4_tests" subclass ooTestCase public

::method init class
  forward class (super) continue
  self~ooTestType = self~DOC_EXAMPLE_NOISE_TEST

::method "test_beep_example_7_4_6"
   -- Note: this only works on some Windows implementations. It is ignored
   --       on Windows 95 and all *nix versions.
   /* C scale */
   note.1 = 262    /* middle C */
   note.2 = 294    /*    D     */
   note.3 = 330    /*    E     */
   note.4 = 349    /*    F     */
   note.5 = 392    /*    G     */
   note.6 = 440    /*    A     */
   note.7 = 494    /*    B     */
   note.8 = 523    /*    C     */
   do i = 1 to 8
      call beep note.i, 250    /* hold each note for one-quarter second */
      ret = RESULT
      self~assertSame("", ret, "Loop:" i "Calling beep should produce a result in RESULT")
      end
   self~assertTrue(i == (8 + 1))


::options novalue error
