  parse source . . s

  group = .TestGroup~new(s)
  group~add(.ABBREV.testGroup)

  if group~isAutomatedTest then return group

  testResult = group~suite~execute~~print

return testResult
-- End of entry point.

::requires 'ooTest.frm'     -- load the ooRexxUnit classes

::class "ABBREV.testGroup" subclass ooTestCase public

::method "test_1"
   self~assertSame(.true, ABBREV('', ''))

::method "test_2"
   self~assertSame(.false, ABBREV('', 'a'))

::method "test_3"
   self~assertSame(.true, ABBREV('a', ''))

::method "test_4"
   self~assertSame(.true, ABBREV('a', 'a'))

::method "test_5"
   self~assertSame(.false, ABBREV('a', 'b'))

::method "test_6"
   s="XRANGE"('00'x,'FF'x)
   Do i=0 To 256
      s30="SUBSTR"(s,1,30)
      t="SUBSTR"(s,1,i)
      self~assertSame(.true, ABBREV(s, t))
      r="ABBREV"(s30,t)
      If i<=30 Then
         self~assertSame(.true, ABBREV(s30, t))
      Else                 /* longer string is never abbrev of shorter */
         self~assertSame(.false, ABBREV(s30, t))
      End

::method "test_7"
   do i=0 to 255
      do j=0 to 255
         s=d2c(i)||d2c(j)                /* s='0000'X...'FFFF'X            */
         t=d2c(j)                        /* t='00'X...'FF'X                */
         self~assertSame((i = j), ABBREV(s, t, 1))
         End
      End

