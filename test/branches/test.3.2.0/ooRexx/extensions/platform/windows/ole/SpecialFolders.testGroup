::requires 'ooTest.frm'
::class 'SpecialFolders.testgroup' subclass ooTestCase public

/*
NOTE:   If the user has changed the name of a "special folder" the following line in each testMethod may fail
        self~assertTrue(objFolderItem~name~caselessPos('....') > 0)

        This is being developed on a system where none of the special folder names have been changed and is
        meant to test that the correct Hex value is being used during the development
*/

::method 'testInternetExplorer'
    objShell = .oleObject~new('Shell.Application')
    sp = '1'~x2d
    objFolder = objShell~nameSpace(sp)
    objFolderItem = objFolder~self
    self~assertSame(objFolderItem~string,objFolder~Self)
    self~assertNotNull(objFolderItem)
    self~assertNotNull(objFolderItem~name)
    self~assertNotNull(objFolderItem~path)
    self~assertTrue(objFolderItem~name~caselessPos('internet') > 0)
    objCollection = objFolder~items
    ctr = 0
    do objItem over objCollection
        ctr += 1
    end
    self~assertEquals(ctr,objCollection~count)

