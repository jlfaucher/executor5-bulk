::requires 'ooTest.frm'

/* class: template.testGroup - - - - - - - - - - - - - - - - - - - - - - - - -*\

\* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/
::class "template01.testGroup" public subclass ooTestCase

-- testMethods for each of the possible assertions
::method 'test01'
-- this testMethod demonstrates the use of assertEquals
    self~assertEquals('01',1)

::method 'test02'
-- this testMethod demonstrates the use of assertNotEquals
    self~assertNotEquals(2,1)

::method 'test03'
-- this testMethod demonstrates the use of assertNull
    self~assertNull(.nil)

::method 'test04'
-- this testMethod demonstrates the use of assertNotNull
    self~assertNotNull('')

::method 'test05'
-- this testMethod demonstrates the use of assertSame
    self~assertSame(1,1*1)

::method 'test06'
-- this testMethod demonstrates the use of assertNotSame
    self~assertNotSame(1,'01')

::method 'test07'
-- this testMethod demonstrates the use of assertTrue
    self~assertTrue(1='01')

::method 'test08'
-- this testMethod demonstrates the use of assertFalse
    self~assertFalse(1=='01')

::method 'test09'
-- this testMethod demonstrates the use of expectSyntax
    self~expectSyntax(42.3)
    rv = 1 / 0

::method 'test10'
-- this testMethod demonstrates the use of expectSyntax
    self~expectCondition("NOSTRING")
    raise NOSTRING





-- End of class: template.testGroup
