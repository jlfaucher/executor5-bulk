::method "test_setMethod_unSetMethod"

  d=.directory~new

  prePostFix="RGF"
  idxVal="rgf"
  itemValue="Rony G. Flatscher"


  m1=.array~of( "use arg name                      ;" -
                "return 'RGF'name~strip~reverse'RGF'")

  d~setMethod("unknown", m1)

  d~setMethod("reverse", "return 'oha, dackel' ")


  d~rgf=itemValue
  self~assertEquals("subtest_01", itemValue, d~rgf)
  self~assertEquals("subtest_02", itemValue, d~entry(idxVal))
  self~assertEquals("subtest_03", itemValue, d~at(idxVal~translate))

  newIdxVal=" nixi "
  expectedVal=prePostFix || newIdxVal~strip~reverse~translate || prePostFix
  self~assertEquals("subtest_04", expectedVal, d~entry(newIdxVal))

  d~setMethod("unknown")      -- unset the method
  self~assertNull("subtest_05", d~entry(newIdxVal))

  testVal='oha, dackel'
  self~assertEquals("subtest_06", testVal, d~reverse)

  d~unSetMethod("REVERSE")
  self~assertNull("subtest_07", d~reverse)




