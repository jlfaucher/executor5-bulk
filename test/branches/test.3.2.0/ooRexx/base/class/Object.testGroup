/* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*\
  Directives, Classes, or Routines.
\* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/
::requires 'ooTest.frm'

/* class: object.testGroup - - - - - - - - - - - - - - - - - - - - - - - - - -*\

\* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -*/
::class "object.testGroup" public subclass ooTestCase

::method test_hasMethod

  o = .object~new

  self~assertTrue(o~hasMethod("SETMETHOD"))
  self~assertTrue(o~hasMethod("UNSETMETHOD"))
  self~assertTrue(o~hasMethod("HASMETHOD"))

::method  test_hasMethod_two

  o = .object~new
  self~assertTrue(o~hasMethod("UnSetMethod"))
  self~assertTrue(o~hasMethod("init"))
  self~assertTrue(o~hasMethod("Run"))

::method test_hasMethod_not

  o = .object~new

  self~assertFalse(o~hasMethod(NEW))
  self~assertFalse(o~hasMethod('NEW'))
  self~assertFalse(o~hasMethod('NeW'))
  self~assertFalse(o~hasMethod('new'))

  self~assertFalse(o~hasMethod("LENGTH"))
  s = .string~new("hello")
  self~assertTrue(s~hasMethod("length"))

-- End of class: object.testGroup
