::method "test_DO_standardTest2E"
   i=0;
   Do 7       ; i=i+1; End;                  /* exprr = constant         */
   self~AssertSame("subTest1", i,7)
   i=0; exprr=7;
   Do exprr   ; i=i+1; End;                  /* exprr = variable         */
   self~AssertSame("subTest2", i,7)
   i.=0; exprr.4=4;exprr.5=5;exprr.6=6;
   Do j=4 to 6
      Do exprr.j ; i.j=(i.j)+1; End;         /* exprr = compound variable*/
   End
   self~AssertSame("subTest3", (i.4 i.5 i.6),(4 5 6))
   i=0;
   Do ((7*exprr)+1)/exprr.5 ; i=i+1; End;    /* exprr = expression       */
   self~AssertSame("subTest4", i,10)

::method "test_DO_standardTest2F"
   i=0;
   Do 7  While i<5 ; i=i+1; End;                       /* exprw=constant */
   self~AssertSame("subTest1", i,5)
   i=0; exprw=5
   Do 7  While i<exprw ; i=i+1; exprw=exprw+1; End;    /* exprr=variable */
   self~AssertSame("subTest2", (i exprw),(7 12))
   i.=0; exprw.4=4;exprw.5=5;exprw.6=6;
   Do j=4 to 6
      Do 7  While i.j<exprw.j; i.j=(i.j)+1;End;   /* exprr=compound variable*/
   End
   self~AssertSame("subTest3", (i.4 i.5 i.6),(4 5 6))
   i=0; exprw=5;
   Do 7 While i<((7*exprw)-15)/exprw.5 ; i=i+1; End;  /* exprr=expression*/
   self~AssertSame("subTest4", i,4)
   i=0;
   Do 7  Until i> 5 ; i=i+1; End;                      /* expru=constant */
   self~AssertSame("subTest5", i,6)
   i=0; expru=5
   Do 7  Until i>=expru ; i=i+1; expru=expru+1;End;    /* expru=variable */
   self~AssertSame("subTest6", (i expru),(7 12))
   expru=5
   i.=0; expru.4=4;expru.5=5;expru.6=6;
   Do j=4 to 6
      Do 7  Until i.j>=expru.j; i.j=(i.j)+1; End;  /*expru=compound variable*/
   End
   self~AssertSame("subTest7", (i.4 i.5 i.6),(4 5 6))
   i=0;
   Do 7 Until i>=((7*expru)-15)/expru.5 ; i=i+1; End; /* expru=expression*/
   self~AssertSame("subTest8", i,4)

