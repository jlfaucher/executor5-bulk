::class "DO.testGroup" subclass TestCase public

::method "test_DO"
  i=0
  DO
     i=i+1
     if i>3 then leave
  end
  self~assertEquals("subtest_01", 1, i)


::method "test_DO_4"
  i=0
  DO 4
     i+=1
  end
  self~assertEquals("subtest_01", 4, i)

::method "test_DO_a=4"
  i=0
  a=4
  DO a
     i+=1
  end
  self~assertEquals("subtest_01", a, i)


::method "test_DO_WHILE_i<4"
  i=0
  DO while i<4
     i+=1
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_WHILE_.true, .true, i<4"
  i=0
  DO while .true, .true, i<4
     i+=1
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_UNTIL_i>3"
  i=0
  DO until i>3
     i+=1
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_UNTIL_.true, .true, i>3"
  i=0
  DO until .true, .true, i>3
     i+=1
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_i=1"
  DO i=1
     if i=4 then leave
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_i=1_to_3"
  DO i=1 to 3
     nop
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_i=0_to_3_by_2"
  DO i=0 to 3 by 2
     nop
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_i=0_by_2"
  DO i=0 by 2
     if i>=4 then leave
  end
  self~assertEquals("subtest_01", 4, i)


::method "test_DO_i=-4.04 by 1.01"
  DO i=-4.04 by 1.01
     if i>=4 then leave
  end
  self~assertEquals("subtest_01", 4.04, i)


::method "test_DO_i=4.04 by -1.01"
  DO i=4.04 by -1.01
     if i<=-4 then leave
  end
  self~assertEquals("subtest_01", -4.04, i)



::method "test_DO_LABEL"
      -- test 1
   i=0
   DO label aha1
      self~assertTrue("subtest_01: LABEL subkeyword on DO known", .true)
      i=i+1
      DO label aha2
         self~assertTrue("subtest_02: LABEL subkeyword on DO known", .true)
         leave aha2
         self~fail("subtest_03: 'leave aha2' did not work!")
      end
      i=i+1
      leave aha1
   END aha1

   self~assertEquals("subtest_04: leaving 'aha2'!", 2, i)

      -- test 2
   i=0
   DO label aha1
      self~assertTrue("subtest_05: LABEL subkeyword on DO known", .true)
      i=i+1
      DO label aha2
         self~assertTrue("subtest_06: LABEL subkeyword on DO known", .true)
         leave aha1
         self~fail("subtest_07: 'leave aha2' did not work!")
      end aha2
      i=i+1
   END aha1

   self~assertEquals("subtest_08: leaving 'aha1'!", 1, i)


      -- test 3
   i=0
   DO label aha1
      self~assertTrue("subtest_09: LABEL subkeyword on DO known", .true)
      i=i+1
      DO i=1 to 15
         self~assertTrue("subtest_10: LABEL subkeyword on DO known", .true)
         leave aha1
         self~fail("subtest_11: 'leave aha2' did not work!")
      end i
      i=i+1
      leave
   END aha1

   self~assertEquals("subtest_12: leaving 'aha1'!", 1, i)

     -- test 4
   str="i=0                                                                    ;" -
   "DO label aha1                                                              ;" -
   "   self~assertTrue(""subtest_15: LABEL subkeyword on DO known"", .true)    ;" -
   "   i=i+1                                                                   ;" -
   "   DO label aha2                                                           ;" -
   "      self~assertTrue(""subtest_16: LABEL subkeyword on DO known"", .true) ;" -
   "      leave aha2                                                           ;" -
   "      self~fail(""subtest_17: 'leave aha2' did not work!"")                ;" -
   "   end                                                                     ;" -
   "   i=i+1                                                                   ;" -
   "END aha1                                                                   ;" -
   "self~assertEquals(""subtest_18: leaving 'aha2'!"", 2, i)                   ;"

   interpret str


