   -- test the BIF, using examples from the documentation
::method "test_VERIFY"
    self~assertEquals(0, .mutablebuffer~new('123')~verify('1234567890'))
    self~assertEquals(2, .mutablebuffer~new('1Z3')~verify('1234567890'))
    self~assertEquals(1, .mutablebuffer~new('AB4T')~verify('1234567890'))
    self~assertEquals(3, .mutablebuffer~new('AB4T')~verify('1234567890','M'))
    self~assertEquals(1, .mutablebuffer~new('AB4T')~verify('1234567890','N'))
    self~assertEquals(4, .mutablebuffer~new('1P3Q4')~verify('1234567890', ,3))
    self~assertEquals(2, .mutablebuffer~new('123')~verify('',N,2))  --
    self~assertEquals(3, .mutablebuffer~new('ABCDE')~verify('', ,3))  --
    self~assertEquals(6, .mutablebuffer~new('AB3CD5')~verify('1234567890','M',4))

::method testRange01
    self~assertSame('0', .mutablebuffer~new('')~verify('',,,3))
    self~assertSame('1', .mutablebuffer~new('abc')~verify('',,,0))
    self~assertSame('2', .mutablebuffer~new('abc')~verify('',,2,0))
    self~assertSame('0', .mutablebuffer~new('ab')~verify('a',,,1))
    self~assertSame('0', .mutablebuffer~new('ABCDEF')~verify('ABC',,,3.0))
    self~assertSame('4', .mutablebuffer~new('ABCDEF')~verify('ABC',,2,3))
    self~assertSame('0', .mutablebuffer~new('ABCDEF')~verify('DEF',Match,,3))
    self~assertSame('4', .mutablebuffer~new('ABCDEF')~verify('ADEF',Match,2,3))

