-- test float
::method 'testFloat01'
  numeric digits 15
  val = 3.40282347E+38
  self~assertFuzzyEquals(val, TestFloatArg(val), 9)
  val = 1.17549435E-38
  self~assertFuzzyEquals(val, TestFloatArg(val), 9)
  val = 0
  self~assertFuzzyEquals(val, TestFloatArg(val), 9)
  val = 1
  self~assertFuzzyEquals(val, TestFloatArg(val), 9)
  val = -1
  self~assertFuzzyEquals(val, TestFloatArg(val), 6)
  val = "nan"
  self~assertSame(val, TestFloatArg(val))
  val = "+infinity"
  self~assertSame(val, TestFloatArg(val))
  val = "-infinity"
  self~assertSame(val, TestFloatArg(val))
  val = 3.40282347E+40
  self~assertSame("+infinity", TestFloatArg(val))
  val = -3.40282347E+40
  self~assertSame("-infinity", TestFloatArg(val))
  val = 3.40282347E-50
  self~assertFuzzyEquals(0, TestFloatArg(val), 9)
  val = -3.40282347E-50
  self~assertFuzzyEquals(0, TestFloatArg(val), 9)

