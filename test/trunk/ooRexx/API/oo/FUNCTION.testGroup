  val = 1.79769313E+308
  self~assertSame(val, TestDoubleArg(val))
  val = 2.22507386E-308
  self~assertSame(val, TestDoubleArg(val))
  val = 0
  self~assertSame(val, TestDoubleArg(val))
  val = 1
  self~assertSame(val, TestDoubleArg(val))
  val = -1
  self~assertSame(val, TestDoubleArg(val))
  val = "nan"
  self~assertSame(val, TestDoubleArg(val))
