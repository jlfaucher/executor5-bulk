-- test int
::method 'testint01'
  numeric digits 11
  val = 0
  self~assertSame(val, TestIntArg(val))
  val = 2147483647
  self~assertSame(val, TestIntArg(val))
  val = 1
  self~assertSame(val, TestIntArg(val))
  val = -1
  self~assertSame(val, TestIntArg(val))
  val = -2147483647
  self~assertSame(val, TestIntArg(val))
  self~assertSame(1,TestIntArg(1.0))
  self~assertSame(1,TestIntArg(1e0))

