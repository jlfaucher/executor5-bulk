-- test float
::method 'testFloat01'
  numeric digits 15
  val = 3.40282347E+38
  self~assertSame(val, TestFloatArg(val))
  val = 1.17549435E-38
  self~assertSame(val, TestFloatArg(val))
  val = 0
  self~assertSame(val, TestFloatArg(val))
  val = 1
  self~assertSame(val, TestFloatArg(val))
  val = -1
  self~assertSame(val, TestFloatArg(val))
  val = "nan"
  self~assertSame(val, TestFloatArg(val))
  val = "+infinity"
  self~assertSame(val, TestFloatArg(val))
  val = "-infinity"
  self~assertSame(val, TestFloatArg(val))
  val = 3.40282347E+40
  self~assertSame("+infinity", TestFloatArg(val))
  val = -3.40282347E+40
  self~assertSame("-infinity", TestFloatArg(val))
  val = 3.40282347E-50
  self~assertSame(0, TestFloatArg(val))
  val = -3.40282347E-50
  self~assertSame(0, TestFloatArg(val))

::method 'testFloat02'
  self~expectSyntax('88.921')
  TestFloatArg('abc')

-- test double
::method 'testDouble01'
