-- test float
::method 'test043'
  numeric digits 15
  val = 3.40282347E+38
  self~assertSame(val, tstargs022(val))
  val = 1.17549435E-38
  self~assertSame(val, tstargs022(val))
  val = 0
  self~assertSame(val, tstargs022(val))
  val = 1
  self~assertSame(val, tstargs022(val))
  val = -1
  self~assertSame(val, tstargs022(val))
  val = "nan"
  self~assertSame(val, tstargs022(val))
  val = "+infinity"
  self~assertSame(val, tstargs022(val))
  val = "-infinity"
  self~assertSame(val, tstargs022(val))
  val = 3.40282347E+40
  self~assertSame("+infinity", tstargs022(val))
  val = -3.40282347E+40
  self~assertSame("-infinity", tstargs022(val))

