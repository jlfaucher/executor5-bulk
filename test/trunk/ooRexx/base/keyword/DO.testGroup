::class "DO.testGroup" subclass ooTestCase public

::method "test_DO"
  i=0
  DO
     i=i+1
     if i>3 then leave
  end
  self~assertEquals(1, i)


::method "test_DO_4"
  i=0
  DO 4
     i+=1
  end
  self~assertEquals(4, i)

::method "test_DO_a=4"
  i=0
  a=4
  DO a
     i+=1
  end
  self~assertEquals(a, i)


::method "test_DO_WHILE_i<4"
  i=0
  DO while i<4
     i+=1
  end
  self~assertEquals(4, i)


::method "test_DO_WHILE_.true, .true, i<4"
  i=0
  DO while .true, .true, i<4
     i+=1
  end
  self~assertEquals(4, i)


::method "test_DO_UNTIL_i>3"
  i=0
  DO until i>3
     i+=1
  end
  self~assertEquals(4, i)


::method "test_DO_UNTIL_.true, .true, i>3"
  i=0
  DO until .true, .true, i>3
     i+=1
  end
  self~assertEquals(4, i)


::method "test_DO_i=1"
  DO i=1
     if i=4 then leave
  end
  self~assertEquals(4, i)


::method "test_DO_i=1_to_3"
  DO i=1 to 3
     nop
  end
  self~assertEquals(4, i)


::method "test_DO_i=0_to_3_by_2"
  DO i=0 to 3 by 2
     nop
  end
  self~assertEquals(4, i)


::method "test_DO_i=0_by_2"
  DO i=0 by 2
     if i>=4 then leave
  end
  self~assertEquals(4, i)


::method "test_DO_i=-4.04 by 1.01"
  DO i=-4.04 by 1.01
     if i>=4 then leave
  end
  self~assertEquals(4.04, i)


::method "test_DO_i=4.04 by -1.01"
  DO i=4.04 by -1.01
     if i<=-4 then leave
  end
  self~assertEquals(-4.04, i)



::method "test_DO_LABEL"
      -- test 1
   i=0
   DO label aha1
      self~assertTrue(.true, "subtest_01: LABEL subkeyword on DO known")
      i=i+1
      DO label aha2
         self~assertTrue(.true, "subtest_02: LABEL subkeyword on DO known")
         leave aha2
         self~fail("subtest_03: 'leave aha2' did not work!")
      end
      i=i+1
      leave aha1
   END aha1

   self~assertEquals(2, i, "subtest_04: leaving 'aha2'!")

      -- test 2
   i=0
   DO label aha1
      self~assertTrue(.true, "subtest_05: LABEL subkeyword on DO known")
      i=i+1
      DO label aha2
         self~assertTrue(.true, "subtest_06: LABEL subkeyword on DO known")
         leave aha1
         self~fail("subtest_07: 'leave aha2' did not work!")
      end aha2
      i=i+1
   END aha1

   self~assertEquals(1, i, "subtest_08: leaving 'aha1'!")


      -- test 3
   i=0
   DO label aha1
      self~assertTrue(.true, "subtest_09: LABEL subkeyword on DO known")
      i=i+1
      DO i=1 to 15
         self~assertTrue(.true, "subtest_10: LABEL subkeyword on DO known")
         leave aha1
         self~fail("subtest_11: 'leave aha2' did not work!")
      end i
      i=i+1
      leave
   END aha1

   self~assertEquals(1, i, "subtest_12: leaving 'aha1'!")


::method test_DO_counter

  do counter i k = 1 to 3
  end

  self~assertEquals(3, i)
  self~assertEquals(4, k)

  do counter i k = 3 to 1
  end

  self~assertEquals(0, i)
  self~assertEquals(3, k)

  do label abc counter i k = 1 to 3
  end abc

  self~assertEquals(3, i)
  self~assertEquals(4, k)

  do counter i label abc k = 3 to 1
  end abc

  self~assertEquals(0, i)
  self~assertEquals(3, k)

  do counter = 1 to 3
  end

  self~assertEquals(4, counter)

  -- uses the counter variable in the condition
  do counter i while i < 3
  end

  self~assertEquals(3, i)

  -- uses the counter variable in the condition
  do counter i until i > 3
  end

  self~assertEquals(4, i)

  -- uses the counter variable in the condition
  do counter i while .false
  end

  self~assertEquals(0, i)

  -- uses the counter variable in the condition
  do counter i until .true
  end

  self~assertEquals(1, i)

  do counter i 3
  end

  self~assertEquals(3, i)

  do counter i item over (1,2,3)
  end

  self~assertEquals(3, i)
  self~assertEquals(3, item)

  do counter i with item item index index over (4,5,6)
  end

  self~assertEquals(3, i)
  self~assertEquals(3, index)
  self~assertEquals(6, item)

  do counter i forever while i < 3
  end

  self~assertEquals(3, i)

  do counter i forever until i > 3
  end

  self~assertEquals(4, i)

  do counter i forever while .false
  end

  self~assertEquals(0, i)

  do counter i forever until .true
  end

  self~assertEquals(1, i)

::method test_simple_do_no_counter
  self~expectSyntax(27.905)
  interpret "do counter i; end"

::method test_multiple_counter
  -- this gets parsed as FOR count loop, with an invalid
  -- count for the second occurrence.
  self~expectSyntax(26.2)
  interpret "do counter x counter y while .true; end"


::method "test_DO_label_works_with_interpret"
   str="i=0                                                                    ;" -
   "DO label aha1                                                              ;" -
   "   i=i+1                                                                   ;" -
   "   DO label aha2                                                           ;" -
   "      leave aha2                                                           ;" -
   "      i = 9                                                                ;" -
   "   end                                                                     ;" -
   "   i=i+1                                                                   ;" -
   "END aha1                                                                   ;" -

   interpret str

   self~assertNotEquals(0, i, "i must be incremented")
   self~assertNotEquals(10, i, "Leave aha2 did not work")
   self~assertEquals(2, i, "Do loops with label must leave i == 2")


::method "test_DO_LABEL_interpret_with_error"

   -- This test case should produce a synatx error because the label on the END
   -- keyword must match the label on the matching DO.  Test just that and
   -- nothing else.

   str="i=0                                                                    ;" -
   "DO label aha1                                                              ;" -
   "   i=i+1                                                                   ;" -
   "   do label aha2                                                           ;" -
   "      leave aha2                                                           ;" -
   "   end                                                                     ;" -
   "   i=i+1                                                                   ;" -
   "   leave                                                                   ;" -
   "END nixi                                                                   ;" -

   self~expectSyntax(10.2)
   interpret str

