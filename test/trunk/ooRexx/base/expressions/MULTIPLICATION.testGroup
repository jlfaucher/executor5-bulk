::method "test_bug1339" -- [bugs:#1339] Multiplication gives wrong result 
  numeric digits 100 
  do m = 2 to 3
    n=1
    do i=1 to 65
      n = n * m
      self~assertTrue(n * n * n >= 4) -- can never be less than 4
    end
  end
  self~assertSame(435600*435600*435600*660, 54551607010560000000)
  self~assertSame(65536*65536*65536*65536, 18446744073709551616)

::method 'test_15_bit'  -- 15-bit edge cases
  numeric digits 100
  self~assertSame(-32768 * -32768, 1073741824)
  self~assertSame(-32768 * -32767, 1073709056)
  self~assertSame(-32768 * -32766, 1073676288)
  self~assertSame(-32768 * 32767, -1073709056)
  self~assertSame(-32768 * 32768, -1073741824)
  self~assertSame(-32768 * 32769, -1073774592)
  self~assertSame(-32767 * -32768, 1073709056)
  self~assertSame(-32767 * -32767, 1073676289)
  self~assertSame(-32767 * -32766, 1073643522)
  self~assertSame(-32767 * 32767, -1073676289)
  self~assertSame(-32767 * 32768, -1073709056)
  self~assertSame(-32767 * 32769, -1073741823)
  self~assertSame(-32766 * -32768, 1073676288)
  self~assertSame(-32766 * -32767, 1073643522)
  self~assertSame(-32766 * -32766, 1073610756)
  self~assertSame(-32766 * 32767, -1073643522)
  self~assertSame(-32766 * 32768, -1073676288)
  self~assertSame(-32766 * 32769, -1073709054)
  self~assertSame(32767 * -32768, -1073709056)
  self~assertSame(32767 * -32767, -1073676289)
  self~assertSame(32767 * -32766, -1073643522)
  self~assertSame(32767 * 32767, 1073676289)
