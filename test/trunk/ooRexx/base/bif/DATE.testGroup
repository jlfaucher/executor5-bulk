::method 'test05'
    day.00='Sunday'
    day.01='Monday'
    day.02='Tuesday'
    day.03='Wednesday'
    day.04='Thursday'
    day.05='Friday'
    day.06='Saturday'

    month.01='January'
    month.02='February'
    month.03='March'
    month.04='April'
    month.05='May'
    month.06='June'
    month.07='July'
    month.08='August'
    month.09='September'
    month.10='October'
    month.11='November'
    month.12='December'

    x = 0

    validopts = 'bdemnosuwBDEMNOSUW'

    do while x < 256
        a = d2c(x)
        if verify(a,validopts) then
            goodopt = .false
        else
            goodopt = .true

        dx = date()
        dy = date()
        do i = 1 to length(validopts) / 2
            dx ||= '*'date(substr(validopts,i,1))
            dy ||= '*'date(translate(substr(validopts,i,1)))
        end

        parse var dx d0 '*' db '*' dd '*' de '*' dm '*' dn '*' do '*' ds '*' du '*' dw
        parse var d0 d0dd d0mon d0yyyy
        parse var de dedd '/' demm '/' deyy
        parse var do doyy '/' domm '/' dodd
        parse var ds dsyy 5  dsmm 7 dsdd
        parse var du dumm '/' dudd '/' duyy
        year  = dsyy
        month = dsmm
        day   = dsdd

        pdays = 0
        do i = (year % 100) * 100 to year - 1
            if leapyear(i) then
                pdays += 366
            else
                pdays += 365
        end

        cdays = 0
        do i = 1 to month-1
            if i= 1 | i = 3 | i = 5 | i = 7 | i = 8 | i = 10 | i = 12 then
                cdays += 31
            else
                if i = 2 then
                    if leapyear(year) then
                        cdays += 29
                    else
                        cdays += 28
                else
                    cdays += 30
        end

        cdays += day
        days = pdays + cdays

        self~assertSame('subTestA'||x~right(3,'0'),dy,dx)

        self~assertSame('subTestB'||x~right(3,'0'),d0dd,strip(day,'l',0))
        self~assertSame('subTestC'||x~right(3,'0'),d0mon,SUBSTR(month.month,1,3))
        self~assertSame('subTestD'||x~right(3,'0'),d0yyyy,year)
        self~assertSame('subTestE'||x~right(3,'0'),cdays,dd)
        self~assertSame('subTestF'||x~right(3,'0'),right(year//100,2,0),deyy)
        self~assertSame('subTestG'||x~right(3,'0'),month,demm)
        self~assertSame('subTestH'||x~right(3,'0'),day,dedd)
        self~assertSame('subTestI'||x~right(3,'0'),month.month,dm)
        self~assertSame('subTestJ'||x~right(3,'0'),year,dsyy)
        self~assertSame('subTestK'||x~right(3,'0'),month,dsmm)
        self~assertSame('subTestL'||x~right(3,'0'),day,dsdd)
        self~assertSame('subTestM'||x~right(3,'0'),right(year//100,2,0),duyy)
        self~assertSame('subTestN'||x~right(3,'0'),month,dumm)
        self~assertSame('subTestO'||x~right(3,'0'),day,dudd)

        wdi = right((days-2)//7,2,'0')
        self~assertSame('subTestP'||x~right(3,'0'),day.wdi,dw)
        self~assertSame('subTestQ'||x~right(3,'0'),date('n'),date())
        x += 1
    end

