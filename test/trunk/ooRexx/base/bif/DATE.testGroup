::requires 'ooTest.frm'     -- load the ooRexxUnit classes

::class "DATE.testGroup" subclass ooTestCase public

::method 'test01'
    self~expectSyntax(40.904)
    date('*')

::method 'test02'
    self~expectSyntax(40.904)
    date('Z')

::method 'test03'
    validopts = 'bdemnosuwBDEMNOSUW'
    do i = 1 to length(validopts) / 2
        a = substr('*@A1B2C3D4EFGHIJKLMNOPQRSTUVWXYZ',i,i)
        b = substr(validopts,i,1)
        c = translate(b)
        self~assertSame(date(b||a), date(b))
    end

::method 'test04'
    validopts = 'bdemnosuwBDEMNOSUW'
