::method 'test03'
    validopts = 'bdemnosuwBDEMNOSUW'
    do i = 1 to length(validopts) / 2
        a = substr('*@A1B2C3D4EFGHIJKLMNOPQRSTUVWXYZ',i,i)
        b = substr(validopts,i,1)
        c = translate(b)
        self~assertSame('subTest'i~right(2,'0'),date(b||a),date(b))
    end

