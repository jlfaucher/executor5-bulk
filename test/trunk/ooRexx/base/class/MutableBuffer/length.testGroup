    self~assertSame(mb~length(),(17+4)~length())

::method 'test019'
    numeric digits 9
    mb = .mutablebuffer~new(+10000000000000000000000000000000000)
    self~assertSame('14', mb~length())
    self~assertSame(mb~length(),(+10000000000000000000000000000000000)~length())

::method 'test020'
    mb = .mutablebuffer~new('+10000000000000000000000000000000000')
    self~assertSame('36', mb~length())
    self~assertSame(mb~length(),'+10000000000000000000000000000000000'~length())

::method 'test021'
    len1 = 1
    numeric digits 9
    mb = .mutablebuffer~new(1.0000000001**1000000)
    self~assertSame(len1, mb~length())
    self~assertSame(mb~length(),1.0000000001**1000000~length())

::method 'test022'
    mb = .mutablebuffer~new(1111)
    self~assertSame('4', mb~length())
    self~assertSame(mb~length(),1111~length())

::method 'test023'
    mb = .mutablebuffer~new('1111')
    self~assertSame('4', mb~length())
    self~assertSame(mb~length(),'1111'~length())

::method 'test024'
    mb = .mutablebuffer~new('f1f1f1f1'x)
    self~assertSame('4', mb~length())
    self~assertSame(mb~length(),'f1f1f1f1'x~length())

::method 'test025'
    mb = .mutablebuffer~new(...)
    self~assertSame('3', mb~length())
    self~assertSame(mb~length(),...~length())

::method 'test030'
    mb = .mutablebuffer~new('abcdefgh')
    self~assertSame(8, mb~length())
    self~assertSame(mb~length(),'abcdefgh'~length())

::method 'testLength'
   b = .mutableBuffer~new("1234")
   self~assertEquals(4, b~length, "Wrong length")

   b = .mutableBuffer~new("123")
   self~AssertEquals(3, b~length, "Length incorrect before")

   b~append("abc")
   self~AssertEquals(6, b~length, "Length incorrect after")

   b = .mutableBuffer~new("123456")
   self~AssertEquals(6, b~length, "Length incorrect before")

   b = .mutableBuffer~new("123456")
   b~insert("abc")
   b~insert("abc",7,,"+")
   b~insert("abc",1,0,"+")
   self~AssertEquals(12, b~length, "Length incorrect after")

   b = .mutableBuffer~new("1234567890")
   self~AssertEquals(10, b~length, "Length incorrect before")
   b~delete(11)
   b~delete(1,2)
   self~AssertEquals(8, b~length, "Length incorrect after")


::options novalue error
