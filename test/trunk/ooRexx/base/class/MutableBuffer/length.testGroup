  parse source . . fileSpec
  group = .TestGroup~new(fileSpec)

  group~addWithSuite(.length.testGroup, .lengthSuite)

  if group~isAutomatedTest then return group

  testResult = group~suite~execute~~print

return testResult
-- End of entry point.

::requires 'ooTest.frm' -- load the ooRexxUnit classes

::class 'lengthSuite' public subclass ooTestSuite
::method setUp
    .local~v1 = 'a'
    .local~v30 = copies('8',28)||'  '
    .local~v255 = xrange('01'x,'ff'x)
    .local~v256 = xrange('88'x,'87'x)
    .local~v32768 = copies('*',32768)
    .local~v100000 = right('z',100000)

::method tearDown
    .local~remove("V1")
    .local~remove("V30")
    .local~remove("V255")
    .local~remove("V256")
    .local~remove("V32768")
    .local~remove("V100000")


::class "length.testGroup" subclass ooTestCase public

::method 'test001'
    mb = .mutablebuffer~new('')
    self~assertSame('0', mb~length())
    self~assertSame(mb~length(),''~length())

::method 'test002'
    mb = .mutablebuffer~new(''x)
    self~assertSame('0', mb~length())
    self~assertSame(mb~length(),''x~length())

::method 'test0003'
