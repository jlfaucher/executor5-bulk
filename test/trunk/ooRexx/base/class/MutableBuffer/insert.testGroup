  parse source . . s
  group = .TestGroup~new(s)
  group~add(.insert.testGroup)

  if group~isAutomatedTest then return group

  testResult = group~suite~execute~~print('The Mutable Buffer class.  MutableBuffer::insert method test.', 3)

return testResult

