::method "test_6"
   s="XRANGE"('00'x,'FF'x)
   Do i=0 To 256
      s30="SUBSTR"(s,1,30)
      t="SUBSTR"(s,1,i)
      self~assertSame("subTest6-"i, .true, s~caselessAbbrev(t))
      r="ABBREV"(s30,t)
      If i<=30 Then
         self~assertSame("subTest6-"i, .true, s30~caselessAbbrev(t))
      Else                 /* longer string is never abbrev of shorter */
         self~assertSame("subTest6-"i, .false, s30~caselessAbbrev(t))
      End

