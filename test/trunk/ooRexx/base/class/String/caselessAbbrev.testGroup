  parse source . . s
  group = .TestGroup~new(s)
  group~add(.caselessAbbrev.testGroup)

  if group~isAutomatedTest then return group

  testResult = group~suite~execute~~print

return testResult

::requires 'ooTest.frm' -- load the ooRexxUnit classes

::class "caselessAbbrev.testGroup" subclass ooTestCase public

   -- test the BIF, using examples from the documentation
::method "test_ABBREV_caseless"

   self~assertTrue('Print'~caselessABBREV("Pri"))

   self~assertTrue("PRINT"~caselessABBREV("Pri"))
   self~assertFalse("PRINT"~caselessABBREV("PRI", 4))
   self~assertFalse("PRINT"~caselessABBREV("PRY"))
   self~assertTrue("PRINT"~caselessABBREV(""))
   self~assertFalse("PRINT"~caselessABBREV("", 1))

::method "test_1"
   self~assertSame(.true, ''~caselessAbbrev(''))

::method "test_2"
   self~assertSame(.false, ''~caselessAbbrev('a'))

::method "test_3"
   self~assertSame(.true, 'a'~caselessAbbrev(''))

::method "test_4"
   self~assertSame(.true, 'a'~caselessAbbrev('a'))

::method "test_5"
   self~assertSame(.false, 'a'~caselessAbbrev('b'))

::method "test_6"
   s="XRANGE"('00'x,'FF'x)
   Do i=0 To 256
      s30="SUBSTR"(s,1,30)
      t="SUBSTR"(s,1,i)
      self~assertSame(.true, s~caselessAbbrev(t))
      r="ABBREV"(s30,t)
      If i<=30 Then
         self~assertSame(.true, s30~caselessAbbrev(t))
      Else                 /* longer string is never abbrev of shorter */
         self~assertSame(.false, s30~caselessAbbrev(t))
      End

