::method "test_20"
   s="XRANGE"('00'x,'FF'x)
   Do i=0 To 256
      s30="SUBSTR"(s,1,30)
      t="SUBSTR"(s,1,i)
      i1="MAX"(i-1,0)
      i2=i+1
      self~assertSame(.true, s~caselessAbbrev(t, i1))
      self~assertSame(.true, s~caselessAbbrev(t, i))
      self~assertSame(.false, s~caselessAbbrev(t, i2))
      If 15<=i & i<=30 Then
         self~assertSame(.true, s30~caselessAbbrev(t, 15))
      Else
         self~assertSame(.false, s30~caselessAbbrev(t, 15))
      End

