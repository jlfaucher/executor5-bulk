    xre = '123456789'~contains('34',1,'xxx')

::method testRange03
    self~expectSyntax(93.923)
    xre = '--------'~contains('-',1,-12)

::method testRange04
    self~expectSyntax(93.923)
    xre = '--------'~contains('-',1,0.5)

