-- from bif.testgroup
   -- test the BIF, using examples from the documentation
::method "test_POS"
    self~assertEquals(1, 'Saturday'~contains('day'))
    self~assertEquals(0, 'abc def ghi'~contains('x'))
    self~assertEquals(1, 'abc def ghi'~contains(' '))
    self~assertEquals(1, 'abc def ghi'~contains(' ',5))

   -- new test
    self~assertEquals(0, 'abc def ghi'~contains('',5))

::method testRange01
    self~assertSame('0', ''~contains('',,100))
    self~assertSame('0', ''~contains('',,0))
    self~assertSame('0', 'abcdeeeeef'~contains('e',,4))
    self~assertSame('0', 'abcdeeeeef'~contains('e',1,4))
    self~assertSame('1', 'abcdeeeeef'~contains('e',2,4))
    self~assertSame('0', 'abcdeeeeef'~contains('eee',5,2))

::method testRange02
