-- from bif.testgroup
   -- test the BIF, using examples from the documentation
::method "test_SUBWORD"
    self~assertSameArray('is the', 'Now is the time'~subWords(2,2))
    self~assertSameArray('the time', 'Now is the time'~subWords(3))
    self~assertSameArray("", 'Now is the time'~subWords(5))

   -- new tests
    self~assertSameArray('', 'Now is the time'~subWords(2,0))
    self~assertSameArray('the  time', 'Now  is  the  time  '~subWords(3))
    self~assertSameArray("time", 'Now  is  the  time  '~subWords(4))
    self~assertSameArray("", 'Now  is  the  time  '~subWords(5))

::method assertSameArray
  use arg expected, received
  if expected~isa(.String) then do
      source = expected
      expected = .array~new
      do i = 1 to words(source)
          expected~append(source~word(i))
      end
  end

  self~assertEquals(expected~items, received~items)

  do i = 1 to expected~items
      self~assertEquals(expected[i], received[i])
  end

