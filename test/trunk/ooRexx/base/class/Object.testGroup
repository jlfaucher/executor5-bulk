::method test_objectname
  o = .object~new
  -- the default string value is the defaultname
  self~assertEquals(o~defaultname, o~string)
  o~objectName = "My Test Object"
  -- this changes the string value
  self~assertEquals("My Test Object", o~string)

  -- object copy also copies the internal variables.
  o2 = o~copy
  self~assertEquals(o~string, o2~string)

  -- should have not connection between the two.
  o2~objectname = "Your Test Object"
  -- the copy should change, the original should not.
  self~assertEquals("Your Test Object", o2~string)
  self~assertEquals("My Test Object", o~string)

