::method test_setmethod
  t = .setTester~new
  -- test each of the variations of method type.  We'll override the original
  -- method on each set
  t~testSet('FOOBAR', 'return 123')
  self~assertTrue(t~hasMethod("foobar"))
  self~assertIsA(t~instanceMethod("foobar"), .Method)
  self~assertEquals(123, t~foobar)
  t~testSet('FOOBAR', .array~of('return 456'))
  self~assertEquals(456, t~foobar)
  t~testSet('FOOBAR', .method~new("test", 'return 789'))
  self~assertEquals(789, t~foobar)
  t~testSet('FOOBAR')
  self~assertFalse(t~hasMethod("foobar"))
  self~assertSame(.nil, t~instanceMethod("foobar"))

  self~assertTrue(t~hasMethod("testOverride"))
  self~assertEquals(123, t~testOverride)
  -- this should remove the method
  t~testSet('TESTOVERRIDE')
  self~assertFalse(t~hasMethod("testOverride"))
  t~testSet('TestOverride', 'return 456')
  self~assertTrue(t~hasMethod("testOverride"))
  self~assertEquals(456, t~testOverride)
  t~testUnset('testOverRide')
  self~assertTrue(t~hasMethod("testOverride"))
  self~assertEquals(123, t~testOverride)

  -- test some copy behaviour.  A setMethod/unsetmethod call
  -- should only affect the target object
  t1 = .setTester~new
  t2 = .setTester~new
  t1~testSet('TestOverride', 'return 456')
  self~assertEquals(456, t1~testOverride)
  self~assertEquals(123, t2~testOverride)
  self~assertEquals(123, .setTester~new~testOverride)
  -- a copy of t3 should pick up the new behaviour
  t3 = t1~copy
  self~assertEquals(456, t3~testOverride)
  -- remove this method, and check that all of the objects
  -- still return the correct result
  t1~testUnset('TestOverride')
  self~assertEquals(123, t1~testOverride)
  self~assertEquals(123, t2~testOverride)
  self~assertEquals(456, t3~testOverride)

  -- set that unsetmethod only works on method set via
  -- setmethod.
  t2~testunset('testoverride')
  self~assertEquals(123, t2~testOverride)

