::method test_instancemethod
  -- some tests of instancemethod, instancemethods, and method scope
  t = .test2~new

  s = t~instanceMethods(.test2)
  loop while s~available
     name = s~index
     method = s~item
     self~assertSame(.test2, method~scope)
     self~assertSame(method, t~instanceMethod(name))
     self~assertSame(method, .test2~method(name))
     s~next
  end

  -- because there is an override, we need to get an instance of the
  -- superclass for comparing methods
  t2 = .test1~new

  -- go one level down the hierarchy
  s = t~instanceMethods(.test1)
  loop while s~available
     name = s~index
     method = s~item
     self~assertSame(.test1, method~scope)
     self~assertSame(method, t2~instanceMethod(name))
     self~assertSame(method, .test1~method(name))
     s~next
  end

  -- now get all of the methods and compare them with what the
  -- class reports.

  instanceMethods = .relation~new
  instanceSupplier = t~instanceMethods

  loop while instanceSupplier~available
     instanceMethods~put(instanceSupplier~index, instanceSupplier~item)
     instanceSupplier~next
  end

  classMethods = .relation~new
  classSupplier = .test2~methods

  loop while classSupplier~available
     classMethods~put(classSupplier~index, classSupplier~item)
     classSupplier~next
  end

  self~assertTrue(instanceMethods~equivalent(classMethods))

::method test_run
  t = .runTester~new

  -- base run tests for the different method sources
  self~assertEquals("Private", t~runMethod("return self~test"))
  self~assertEquals("Private", t~runMethod(.array~of("return self~test")))
  self~assertEquals("Private", t~runMethod(.method~new('test', "return self~test")))

  -- base run tests for the different method sources
  self~assertSame(.runTester, t~runMethod("return super"))
  self~assertSame(.runTester, t~runMethod(.array~of("return super")))
  self~assertSame(.runTester, t~runMethod(.method~new('test', "return super")))

  self~assertSame(.nil, t~runMethod("return .context~executable~scope"))
  self~assertSame(.nil, t~runMethod(.array~of("return .context~executable~scope")))
  self~assertSame(.nil, t~runMethod(.method~new('test', "return .context~executable~scope")))

  -- test the "name" of this unnamed method
  self~assertSame("*UNNAMED*", t~runMethod("return .context~name"))
  self~assertSame("*UNNAMED*", t~runMethod("return .context~stackframes[1]~name"))

  -- just testing single forms of methods from here.

  self~assertSame(.array, t~runMethod1("return arg(1)", .array))
  self~assertEquals("David Ashley", t~runMethod2(("use strict arg arg1, arg2", "return arg1 arg2"), "David", "Ashley"))
  self~assertSame(.array, t~runMethodWith("return arg(1)", .array~of(.array)))
  self~assertEquals("David Ashley", t~runMethodWith(("use strict arg arg1, arg2", "return arg1 arg2"), ("David", "Ashley")))

  self~assertEquals("a 2 b", t~runGeneral(("use strict arg a1 = 1, a2 = 2, a3 = 3", "return a1 a2 a3"), 'individual', 'a',,'b'))
  self~assertEquals("a 2 b", t~runGeneral(("use strict arg a1 = 1, a2 = 2, a3 = 3", "return a1 a2 a3"), 'array', ('a',,'b')))

