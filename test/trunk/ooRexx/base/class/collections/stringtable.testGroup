/* add test cases for of and do over for 12.10.2014 Walter Pachl */

::method "test_of_01"
  d1=.stringtable~of()
  self~assertTrue(d1~isEmpty)

::method "test_of_02"
  self~expectSyntax(93.903)
                              -- Missing argument in method; argument 1 is required.
  d2=.stringtable~of(,(a,b),)


::method "test_of_03"
  self~expectSyntax(93.903)
                              -- Missing argument in method; argument 3 is required.
  d3=.stringtable~of((a,b),(c,d),,(e,f))

::method "test_of_04"
  self~expectSyntax(88.909)
                              -- Method argument 2 must have a string value.
  d=.stringtable~new
  d=.stringtable~of((d,1))

::method "test_of_05"          -- ok
  d3=.stringtable~of((a,b),(c,d),(e,f))
  d4=.stringtable~new
  d4~put(b,a); d4~put(d,c); d4~put(f,e)
  d5=.stringtable~new
  d5[a]=b; d5[c]=d; d5[e]=f
  d6=.stringtable~new
  d6~a=b; d6~c=d; d6~e=f
  ilist=.array~of(b,d,f)
  self~assertSame(d3~items,d4~items)
  Do i=1 To ilist~items
    ix=ilist[i]
    self~assertSame(d4[ix],d3[ix])
    self~assertSame(d5[ix],d4[ix])
    self~assertSame(d6[ix],d5[ix])
    End

::method "test_of_06"
  self~expectSyntax(88.923)
                              -- OF argument 1 must be a single-dimensional array;
                              -- found "The NIL object".
  d=.stringtable~of(.nil)       -- not an array

::method "test_of_07"
  a=.array~of(x,y)
  d=.stringtable~of(a)
  self~assertSame(y,d[x])

::method "test_of_08"
  self~expectSyntax(88.909)
                              -- Method argument 2 must have a string value.
  a=.array~of(.nil,u)
  d=.stringtable~of(a)

::method "test_of_09"
  a=.array~of(v,.nil)
  d=.stringtable~of(a)          -- an array
  self~assertSame(.nil,d[v])

::method "test_of_10"
  self~expectSyntax(88.924)
                              -- OF argument 1 must be an array with exactly 2 items.
  d=.stringtable~of((1,))       -- index no value

::method "test_of_11"
  self~expectSyntax(88.924)
                              -- OF argument 1 must be an array with exactly 2 items.
  d=.stringtable~of((,2))       -- value, no index

::method "test_of_12"
  self~expectSyntax(88.924)
                              -- OF argument 1 must be an array with exactly 2 items.
  d=.stringtable~of((1,2,3))    -- too many items in the array

::method "test_of_13"         -- pairs are applied left to right
  d=.stringtable~of((x,y),(x,y),(x,z))
  self~assertSame(1,d~items)
  self~assertSame(z,d[x])
  d~put(1,2)
  self~assertSame(2,d~items)
  self~assertSame(1,d[2])
  d~remove(x)
  self~assertSame(1,d~items)
  self~assertSame(1,d[2])


::method "test_for_01"
  d1=.stringtable~of((a,b),(c,d),(e,f))
  a=.array~new
  loop el over d1 for 2
    a~append(el)
    End
  self~assertSame(2,a~items)

::method "test_for_02"
  el='undefined'
  d2=.stringtable~of((a,b),(c,d),(e,f))
  a=.array~new
  loop el over d2 for 0
    a~append(el)
    End
  self~assertSame(0,a~items)

::method "test_for_03"
  d3=.stringtable~of((a,b),(c,d),(e,f))
  a=.array~new
  loop el over d3 for 10
    a~append(el)
    End
  self~assertSame(3,a~items)

