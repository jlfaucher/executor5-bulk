  parse source . . s
  group = .TestGroup~new(s)
  group~add(.Collection_stringTableMethod.testGroup)

  if group~isAutomatedTest then return group

  testResult = group~suite~execute~~print

return testResult

::requires 'ooTest.frm' -- load the ooRexxUnit classes

::class "Collection_stringTableMethod.testGroup" subclass ooTestCase public

::method "test_1"
   dir = .stringtable~new()
   self~assertEquals('a StringTable', dir)
   self~assertEquals(0, dir~items)
   self~assertEquals(1, dir~isEmpty)

::method "test_2"
   dir = .stringtable~new()
   dir['Mike'] = 'Shrek'
   self~assertEquals(0, dir~isEmpty)
   self~assertEquals(1, dir~items)
   self~assertEquals('Shrek', dir['Mike'])
   arr = .array~new
   dir['Dennis'] = arr
   self~assertEquals(2, dir~items)
   self~assertSame(arr, dir['Dennis'])
   q = .queue~new
   dir~put(q, 'Linda')
   self~assertEquals(3, dir~items)
   self~assertSame(q, dir~at('Linda'))

::method "test_3"
   dir = .stringtable~new()
   dir['Mike'] = 'Shrek'
   dir['Dennis'] = .array~new()
   dir~put(.queue~new(), 'Linda')
   arr = dir~allIndexes()
   self~assertEquals(3, arr~items)
   self~assertTrue(arr~equivalent(.array~of('Mike', 'Dennis', 'Linda')))

::method "test_4"
   dir = .stringtable~new()
   dir~setEntry('Mike', 'Shrek')
   arr = .array~new
   dir['Dennis'] = arr
   queue = .queue~new
   dir~put(queue, 'Linda')
   items = dir~allItems()
   self~assertEquals(3, items~items)
   self~assertTrue(items~equivalent(.array~of('Shrek', arr, queue)))

::method "test_5"
   dir = .stringtable~new()
   dir['Mike'] = 'Shrek'
   dir['Dennis'] = .array~new()
   dir~put(.queue~new(), 'Linda')
   self~assertEquals(3, dir~items)
   dir~empty
   self~assertEquals(0, dir~items)

::method "test_6"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals('Shrek', dir~entry('Mike'))
   self~assertEquals(.nil, dir~entry('Dennis'))
   self~assertEquals('West', dir~entry('David'))
   self~assertEquals(.nil, dir~entry('Linda'))

::method "test_7"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals(1, dir~hasEntry('MIKE'))
   self~assertEquals(0, dir~hasEntry('Dennis'))
   self~assertEquals(1, dir~hasEntry('David'))
   self~assertEquals(0, dir~hasEntry('Linda'))

::method "test_8"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals(1, dir~hasIndex('MIKE'))
   self~assertEquals(1, dir~hasIndex('Dennis'))
   self~assertEquals(0, dir~hasIndex('David'))
   self~assertEquals(0, dir~hasIndex('Linda'))

::method "test_9"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals(1, dir~hasItem('Shrek'))
   self~assertEquals(1, dir~hasItem('West'))
   self~assertEquals(0, dir~hasItem('MILLER'))
   self~assertEquals(0, dir~hasItem('McCall'))

::method "test_10"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   dir['Larry'] = 'Miller'
   self~assertEquals('MIKE', dir~index('Shrek'))
   idx = dir~index('Miller')
   self~assertEquals(1, idx = 'Dennis' | idx = 'Larry')
   self~assertEquals(.nil, dir~index('McCall'))

::method "test_11"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   arr = dir~makeArray()
   self~assertEquals(3, arr~items)
   self~assertEquals(1, arr~hasItem('DAVID'))
   self~assertEquals(1, arr~hasItem('Dennis'))
   self~assertEquals(1, arr~hasItem('MIKE'))

::method "test_12"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals(3, dir~items)
   item = dir~remove('Dennis')
   self~assertEquals(2, dir~items)
   self~assertEquals('Miller', item)

::method "test_13"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals(3, dir~items)
   item = dir~removeItem('Miller')
   self~assertEquals(2, dir~items)
   self~assertEquals('Miller', item)

::method "test_14"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals(3, dir~items)
   dir~setEntry('Mike', 'Humperdink')
   self~assertEquals(3, dir~items)
   self~assertEquals('Humperdink', dir['MIKE'])
   dir~setEntry('David')
   self~assertEquals(2, dir~items)
   dir~setEntry('Dennis')
   self~assertEquals(2, dir~items)

::method "test_16"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   sup = dir~supplier
   self~assertEquals('a Supplier', sup)
   self~assertEquals(1, sup~available)
   if sup~index = 'MIKE' then self~assertEquals('Shrek', sup~item)
   else if sup~index = 'Dennis' then self~assertEquals('Miller', sup~item)
   sup~next
   self~assertEquals(1, sup~available)
   if sup~index = 'MIKE' then self~assertEquals('Shrek', sup~item)
   else if sup~index = 'Dennis' then self~assertEquals('Miller', sup~item)
   sup~next
   self~assertEquals(0, sup~available)
   self~expectSyntax(93.937)
   sup~next

::method "test_17"
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['DAVID'] = 'West'
   self~assertEquals('Shrek', dir~unknown('MIKE', .array~new()))
   dir~unknown('MIKE=', .array~of('Kinder'))
   self~assertEquals('Kinder', dir~unknown('MIKE', .array~new()))

::method "test_18"
   -- xor
   dir = .stringtable~new()
   dir['MIKE'] = 'Shrek'
   dir['Dennis'] = 'Miller'
   dir['Linda'] = 'Miller'
   arr = .array~of('MIKE', 'Dennis')
   -- Note: The array is converted to a bag in the xor method and the
   --       array indexes are lost
   newdir = dir~xor(arr)
   self~assertEquals('Miller', newdir['Linda'])
   self~assertEquals(1, newdir~items)

::method "test_20"
  dir = .stringtable~new

  dir~setEntry('foo', 'bar')
  self~assertTrue(dir~allIndexes~equivalent(.array~of("FOO")))
  self~assertEquals("bar", dir['FOO'])
  self~assertEquals("bar", dir~entry('foo'))
  self~assertTrue(dir~hasentry('foo'))
  self~assertEquals("bar", dir~removeEntry('foo'))
  self~assertSame(.nil, dir['FOO'])

