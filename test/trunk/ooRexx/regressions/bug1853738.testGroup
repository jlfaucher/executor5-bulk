::class "bug1853738.testGroup" public subclass ooTestCase

  ::method test_initClass_property01

    data = "value1,value2,value3,value4,value5,value6,value7,value8,value9,"  || -
           "value10,value11,value12,value13,value14,value15,value16,value17," || -
           "value18,value19,value20"
    a = .helper~new(data)
    self~assertEquals("value1", a~property01, "Class attribute property01 should equal value1")

  ::method test_initClass_property20

    data = "value1,value2,value3,value4,value5,value6,value7,value8,value9,"  || -
           "value10,value11,value12,value13,value14,value15,value16,value17," || -
           "value18,value19,value20"
    a = .helper~new(data)
    self~assertEquals("value20", a~property20, "Class attribute property20 should equal value20")

  ::method test_initClass_property_middlle

    data = "value1,value2,value3,value4,value5,value6,value7,value8,value9,"  || -
           "value10,value11,value12,value13,value14,value15,value16,value17," || -
           "value18,value19,value20"
    a = .helper~new(data)
    self~assertEquals("value10", a~property10, "Class attribute property10 should equal value10")

  ::method test_initClass_property_pastData

    data = "value1,value2,value3,value4,value5,value6,value7,value8,value9,"  || -
           "value10,value11,value12,value13,value14,value15,value16,value17," || -
           "value18,value19,value20"
    a = .helper~new(data)
    self~assertEquals("", a~property21, "Class attribute property21 should equal the empty string")

