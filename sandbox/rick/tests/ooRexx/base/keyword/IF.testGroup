::method "test_34"
   aa.=0                               /* optical check for completeness */
   Do i=1 to 2
      IF  i=1; Then NOP
      Else Do;
         aa.i=aa.i+1
         aa.i=aa.i+1
         End;
      aa.i=aa.i+1
      END
   self~assertSame((aa.1 aa.2), (1 3))

::method "test_35"
   aa.=0
   Do i=1 to 2
      IF  i=1 Then NOP               /* without cw                     */
      Else Do 5 ;
         aa.i=aa.i+1
         End;
      aa.i=aa.i+1
      END
   self~assertSame((aa.1 aa.2), (1 6))
   aa.=0
   Do i=1 to 2
      IF  i=1 Then NOP
      Else Do k=1 To 5 ;     /* with cw                        */
         aa.i=aa.i+1
         End;
      aa.i=aa.i+1
      END
   self~assertSame((aa.1 aa.2), (1 6))

::method "test_36"
    Do i=1 to 4
       tv.=0
       n=1
       If i=1 Then Do; tv.1=1;
          End
       Else Do; tv.1=2
          n=n+1;
          If i=2 Then Do; tv.2=1;
             End;
          Else Do; tv.2=2
             n=n+1
             If i=3 Then Do; tv.3=1;
                End;
             Else Do ; tv.3=2; n=n+1;
                End;
             End;
          End;
       Select
          When i=1 Then self~assertSame((i tv.1 tv.2 tv.3 n), (i 1 0 0 i))
          When i=2 Then self~assertSame((i tv.1 tv.2 tv.3 n), (i 2 1 0 i))
          When i=3 Then self~assertSame((i tv.1 tv.2 tv.3 n), (i 2 2 1 i))
          When i=4 Then self~assertSame((i tv.1 tv.2 tv.3 n), (i 2 2 2 i))
          Otherwise self~assertTrue(0)
          End
       End i

