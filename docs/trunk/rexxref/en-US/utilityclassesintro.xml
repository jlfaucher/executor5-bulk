  <!-- this introduction is included in classes.xml and utilityclasses.xml -->
  <itemizedlist spacing="compact">
   <listitem><para><xref linkend="clsMutableBuffer" xrefstyle="select:title"/>,</para></listitem>
   <listitem><para><xref linkend="clsFile" xrefstyle="select:title"/>,</para></listitem>
   <listitem><para>Date-, time-, and timing-related classes
    <xref linkend="clsDateTime" xrefstyle="template:DateTime"/>,
    <xref linkend="clsTimeSpan" xrefstyle="template:TimeSpan"/>,
    <xref linkend="clsAlarm" xrefstyle="template:Alarm"/>,
    <xref linkend="clsTicker" xrefstyle="template:Ticker"/>,
    and the notification classes
    <xref linkend="clsAlarmNotification" xrefstyle="template:AlarmNotification"/> and
    <xref linkend="clsMessageNotification" xrefstyle="template:MessageNotification"/>,
   </para></listitem>
   <listitem><para>the synchronization classes
    <xref linkend="clsEventSemaphore" xrefstyle="template:EventSempahore"/> and
    <xref linkend="clsMutexSemaphore" xrefstyle="template:MutexSempahore"/>,
   </para></listitem>
   <listitem><para><xref linkend="clsComparable" xrefstyle="template:Comparable"/> class and
    <xref linkend="clsOrderable" xrefstyle="template:Orderable"/> class,
   </para></listitem>
   <listitem><para>eight <xref linkend="clsComparator" xrefstyle="template:Comparator"/> classes used for sorting
    <xref linkend="clsCaselessComparator" xrefstyle="template:(Caseless)"/><xref linkend="clsComparator" xrefstyle="template:Comparator"/>,
    <xref linkend="clsCaselessColumnComparator" xrefstyle="template:(Caseless)"/><xref linkend="clsColumnComparator" xrefstyle="template:ColumnComparator"/>,
    <xref linkend="clsCaselessDescendingComparator" xrefstyle="template:(Caseless)"/><xref linkend="clsDescendingComparator" xrefstyle="template:DescendingComparator"/>,
    <xref linkend="clsInvertingComparator" xrefstyle="template:InvertingComparator"/>, and
    <xref linkend="clsNumericComparator" xrefstyle="template:NumericComparator"/>, and
   </para></listitem>
   <listitem><para>other miscellaneous classes
    <xref linkend="clsBuffer" xrefstyle="template:Buffer"/>,
    <xref linkend="clsMonitor" xrefstyle="template:Monitor"/>,
    <xref linkend="clsPointer" xrefstyle="template:Pointer"/>,
    <xref linkend="clsRegularExpression" xrefstyle="template:RegularExpression"/>,
    <xref linkend="clsRexxContext" xrefstyle="template:RexxContext"/>,
    <xref linkend="clsRexxInfo" xrefstyle="template:RexxInfo"/>,
    <xref linkend="clsRexxQueue" xrefstyle="template:RexxQueue"/>,
    <xref linkend="clsStackFrame" xrefstyle="template:StackFrame"/>,
    <xref linkend="clsStreamSupplier" xrefstyle="template:StreamSupplier"/>,
    <xref linkend="clsSupplier" xrefstyle="template:Supplier"/>,
    <xref linkend="clsValidate" xrefstyle="template:Validate"/>,
    <xref linkend="clsVariableReference" xrefstyle="template:VariableReference"/>, and
    <xref linkend="clsWeakReference" xrefstyle="template:WeakReference"/>.
   </para></listitem>
  </itemizedlist>
